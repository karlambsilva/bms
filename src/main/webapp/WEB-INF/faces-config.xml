<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    		  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    		  xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    		  version="2.2">
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updateBlog</from-outcome>
			<to-view-id>/blog/updateBlog.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
	</navigation-rule>    
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deleteBlog</from-outcome>
			<to-view-id>/blog/deleteBlog.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchBlog</from-outcome>
			<to-view-id>/blog/searchBlog.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>updatePost</from-outcome>
			<to-view-id>/post/updatePost.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
	</navigation-rule>    
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>deletePost</from-outcome>
			<to-view-id>/post/deletePost.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
	</navigation-rule>  
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>searchPost</from-outcome>
			<to-view-id>/post/searchPost.xhtml</to-view-id>
			<redirect></redirect>
		</navigation-case>	
	</navigation-rule>  
			 
</faces-config>
